{
    "name": "proyecto404/coding-standard",
    "description": "404's PHP Code Sniffer Coding Standard",
    "keywords": ["php code sniffer", "coding standard"],
    "homepage": "https://github.com/proyecto404/Proyecto404CodingStandard",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Bottarini",
            "email": "nicolasbottarini@gmail.com",
            "homepage": "http://www.nicolasbottarini.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": { "Proyecto404\\UtilBundle\\": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    }
}